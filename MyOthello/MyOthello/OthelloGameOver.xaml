<Window x:Class="MyOthello.OthelloGameOver"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyOthello"
        mc:Ignorable="d"
        Title="OthelloGameOver" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="winnerNameTxt" HorizontalAlignment="Left" Margin="72,26,0,0" TextWrapping="Wrap" Text="Winner!" VerticalAlignment="Top" Height="104" Width="648" TextAlignment="Center" FontSize="64"/>
        <TextBlock x:Name="winnerScoreTxt" HorizontalAlignment="Left" Margin="261,130,0,0" TextWrapping="Wrap" Text="Score:64" VerticalAlignment="Top" Height="101" Width="271" TextAlignment="Center" FontSize="64"/>
        <Button x:Name="playAgain" Content="Play Again?" HorizontalAlignment="Left" Margin="350,306,0,0" VerticalAlignment="Top" Width="107" Height="29" Click="playAgain_Click"/>

    </Grid>
</Window>
